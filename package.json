{
  "name": "GoTaskerApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env react-native run-android",
    "android-qa": "ENVFILE=.env.qa react-native run-android",
    "android-staging": "ENVFILE=.env.staging react-native run-android",
    "android-prod": "ENVFILE=.env.prod react-native run-android",
    "build-android-qa": "cd android && ./gradlew clean && ENVFILE=.env.qa ./gradlew assembleRelease",
    "build-android-staging": "cd android && ./gradlew clean && ENVFILE=.env.staging ./gradlew assembleRelease",
    "build-android-prod": "cd android && ./gradlew clean && ENVFILE=.env.prod ./gradlew assembleRelease",
    "bundle-android-prod": "cd android && ./gradlew clean && ENVFILE=.env.prod ./gradlew bundleRelease",
    "ios": "ENVFILE=.env react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "ios-qa": "ENVFILE=.env.qa react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "ios-staging": "ENVFILE=.env.staging react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "ios-dev-13Pro": "ENVFILE=.env.dev react-native run-ios --simulator=\"iPhone 13 Pro Max\"",
    "ios-prod": "ENVFILE=.env.prod react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "ios11": "ENVFILE=.env.staging react-native run-ios --simulator=\"iPhone 11\"",
    "ios12": "ENVFILE=.env.staging react-native run-ios --simulator=\"iPhone 12 Pro Max\"",
    "iosx": "ENVFILE=.env.staging react-native run-ios --simulator=\"iPhone Xs Max\"",
    "ios8": "ENVFILE=.env.staging react-native run-ios --simulator=\"iPhone 8\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "clean-android": "cd android && rm -R .gradle && cd app && rm -R build",
    "reconnect": "adb reverse tcp:8081 tcp:8081",
    "menu": "adb shell input keyevent 82",
    "android-dev": "ENVFILE=.env.dev react-native run-android",
    "build-android-dev": "cd android && ./gradlew clean && ENVFILE=.env ./gradlew assembleRelease",
    "ios-dev": "ENVFILE=.env.dev react-native run-ios --simulator=\"iPhone 11 Pro Max\""
  },
